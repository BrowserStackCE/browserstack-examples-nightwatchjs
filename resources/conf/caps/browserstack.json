{
	"project": "browserstack-examples",
	"build": "browserstack-examples-nightwatchjs",
	"server": "hub-cloud.browserstack.com",
	"defaultUrl": "https://bstackdemo.com",

	"android": {
		"os_version": "10.0",
		"device": "Samsung Galaxy S20",
		"real_mobile": "true"
	},

	"iphone": {
		"os_version": "14",
		"device": "iPhone 12",
		"real_mobile": "true"
	},

	"chrome": {
		"browser": "chrome",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		}
	},

	"firefox": {
		"browserName": "firefox",
		"resolution": "1920x1080",
		"browserstack.video": true
	},

	"edge": {
		"browserName": "edge",
		"resolution": "1920x1080",
		"browserstack.video": true
	},

	"safari": {
		"browserName": "safari",
		"resolution": "1920x1080",
		"browserstack.video": true
	},

	"android_local": {
		"os_version": "10.0",
		"device": "Samsung Galaxy S20",
		"real_mobile": "true",
		"browserstack.local": "true"
	},

	"iphone_local": {
		"os_version": "14",
		"device": "iPhone 12",
		"real_mobile": "true",
		"browserstack.local": "true"
	},

	"chrome_local": {
		"browserName": "chrome",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		},
		"browserstack.local": true,
		"browserstack.networkLogs": true,
		"defaultUrl": "http://localhost:3000"
	},

	"firefox_local": {
		"browserName": "firefox",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"browserstack.local": true,
		"defaultUrl": "http://localhost:3000"
	},

	"edge_local": {
		"browserName": "edge",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		},
		"browserstack.local": true,
		"defaultUrl": "http://localhost:3000"
	},

	"safari_local": {
		"browserName": "safari",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.local": true,
		"defaultUrl": "http://bs-local.com:3000"
	}
}
