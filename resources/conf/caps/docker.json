{
	"project": "browserstack-examples",
	"build": "browserstack-examples-nightwatchjs",
	"server": "localhost:4444",
	"chrome": {
		"browserName": "chrome",
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		}
	},

	"firefox": {
		"browserName": "firefox"
	},

	"edge": {
		"browserName": "MicrosoftEdge"
	},

	"safari": {
		"browserName": "safari"
	},

	"opera": {
		"browserName": "operablink"
	}
}
