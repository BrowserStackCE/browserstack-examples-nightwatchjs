{
	"project": "browserstack-examples",
	"build": "browserstack-examples-nightwatchjs",
	"server": "hub-cloud.browserstack.com",
	"defaultUrl": "https://bstackdemo.com",
	"chrome": {
		"browserName": "chrome",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		}
	},

	"firefox": {
		"browserName": "firefox",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN"
	},

	"edge": {
		"browserName": "edge",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN"
	},

	"safari": {
		"browserName": "safari",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN"
	},

	"chrome.local": {
		"browserName": "chrome",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		},
		"browserstack.local": true,
		"browserstack.networkLogs": true,
		"browserstack.localIdentifier": "id",
		"defaultUrl": "http://localhost:3000"
	},

	"firefox.local": {
		"browserName": "firefox",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"browserstack.local": true,
		"browserstack.localIdentifier": "id-firefox",
		"defaultUrl": "http://localhost:3000"
	},

	"edge.local": {
		"browserName": "edge",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"goog:chromeOptions": {
			"prefs": {
				"profile.managed_default_content_settings.geolocation": 1
			}
		},
		"browserstack.local": true,
		"browserstack.localIdentifier": "id-edge",
		"defaultUrl": "http://localhost:3000"
	},

	"safari.local": {
		"browserName": "safari",
		"resolution": "1920x1080",
		"browserstack.video": true,
		"browserstack.geoLocation": "IN",
		"browserstack.local": true,
		"browserstack.localIdentifier": "id-safari",
		"defaultUrl": "http://localhost:3000"
	}
}
