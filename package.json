{
	"name": "browserstack-nightwatchjs",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"test:browserstack": "nightwatch -c resources/conf/runners/browserstack.js --env chrome,edge,safari --parallel",
		"test:browserstack:e2e": "yarn test:browserstack --tag e2e",
		"test:browserstack:offers": "yarn test:browserstack --tag offers",
		"test:browserstack:user": "yarn test:browserstack --tag user",
		"test:browserstack:login": "yarn test:browserstack --tag login",
		"test:browserstack:product": "yarn test:browserstack --tag product",
		"test:docker": "nightwatch -c resources/conf/runners/docker.js --env chrome,edge,opera",
		"test:onprem": "nightwatch -c resources/conf/runners/on-prem.js",
		"test:browserstack:single": "nightwatch -c resources/conf/runners/browserstack.js --env chrome --tag single",
		"test:docker:single": "nightwatch -c resources/conf/runners/browserstack.js --env chrome --tag single",
		"test:onprem:single": "nightwatch -c resources/conf/runners/on-prem.js --tag single",
		"test:browserstack:parallel": "nightwatch -c resources/conf/runners/browserstack.js --env edge --parallel",
		"test:docker:parallel": "nightwatch -c resources/conf/runners/browserstack.js --env chrome --parallel",
		"test:onprem:suite": "nightwatch -c resources/conf/runners/on-prem.js",
		"test:browserstack:parallel_multiple": "nightwatch -c resources/conf/runners/browserstack.js --env chrome,edge,safari --parallel"
	},
	"devDependencies": {
		"chromedriver": "^89.0.0",
		"eslint": "^7.21.0",
		"geckodriver": "^1.22.2",
		"nightwatch": "^1.5.1"
	}
}
