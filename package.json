{
	"name": "browserstack-nightwatchjs",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"test:browserstack": "nightwatch -c resources/conf/caps/nightwatch.browserstack.json --env browserstack.chrome",
		"test:browserstack:offers": "yarn test:browserstack --tag offers",
		"test:browserstack:user": "yarn test:browserstack --tag user",
		"test:browserstack:login": "yarn test:browserstack --tag login",
		"test:browserstack:product": "yarn test:browserstack --tag product",
		"test:self_hosted": "nightwatch -c resources/conf/caps/nightwatch.self_hosted.json --env self_hosted.chrome,self_hosted.firefox --reporter node_modules/nightwatch-allure-adapter/reporter.js"
	},
	"devDependencies": {
		"allure-commandline": "^2.13.8",
		"chromedriver": "^89.0.0",
		"eslint": "^7.21.0",
		"geckodriver": "^1.22.2",
		"nightwatch": "^1.5.1",
		"nightwatch-allure-adapter": "^1.2.4",
		"selenium-server": "^3.141.59"
	}
}
